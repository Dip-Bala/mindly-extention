{
    "manifest_version": 3,
    "name": "Mindly Extension",
    "version": "1.0",
    "permissions": ["storage", "tabs", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://*/*"
    ]
  }
  